import funkin.play.PlayState;
import funkin.play.stage.Stage;

class SchoolStage extends Stage
{
	function new()
	{
		super('school');
	}

	function buildStage()
	{
		super.buildStage();
		getNamedProp('freaks').idleSuffix = (PlayState.instance.currentSong.id.toLowerCase() == "roses") ? '-scared' : '';
	}
}
